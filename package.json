{
  "name": "node-practice",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "postinstall": "typings install",
    "start": "tsc && node ./bin/www",
    "lint": "tslint -e \"./{node_modules,typings}/**/*.ts\" \"./**/*.ts\"",
    "sloc": "sloc --details --format cli-table --exclude \"(node_modules|typings)|(.*)*.js\" .",
    "unit": "tsc && istanbul cover ./node_modules/.bin/mocha tests/unit",
    "test": "npm run lint && npm run sloc && npm run unit"
  },
  "dependencies": {
    "body-parser": "~1.15.1",
    "cookie-parser": "~1.4.3",
    "debug": "~2.2.0",
    "express": "~4.13.4",
    "jade": "~1.11.0",
    "morgan": "~1.7.0",
    "serve-favicon": "~2.3.0"
  },
  "devDependencies": {
    "istanbul": "^0.4.5",
    "mocha": "^3.0.2",
    "mocha-istanbul": "^0.3.0",
    "sloc": "^0.1.11",
    "tslint": "^3.15.1",
    "typescript": "^1.8.10",
    "typings": "^1.3.3"
  }
}
